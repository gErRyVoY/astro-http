{
	"extends": "astro/tsconfigs/strict",
	"include": [
		".astro/types.d.ts",
		"**/*"
	],
	"exclude": [
		"dist"
	],
	"compilerOptions": {
		"strictNullChecks": true,
		"baseUrl": ".",
		"paths": {
			"@db": ["src/db"],
			"@layouts/*": ["src/layouts/*"],
			"@vue/*": ["src/components/likes/*"],
		},
		"jsx": "preserve"
	}
}